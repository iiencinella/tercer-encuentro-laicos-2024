---
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<nav class="bg-white border-gray-200">
  <div
    class="max-w-screen-xl flex flex-wrap items-center justify-center gap-4 mx-auto p-4"
  >
    <img src="/logo.webp" class="h-20 rounded-full" alt="Logo" />
    <span
      class="text-center text-2xl font-semibold"
      >{title}</span
    >
  </div>
</nav>
